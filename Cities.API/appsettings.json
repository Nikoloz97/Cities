 {
  "Logging": {
    "LogLevel": {
      // For default namespace, level needs to be at level info (or higher) to be logged
      "Default": "Information",
      // We can put in specific level thresholds for other namespaces
      "Cityinfo.API.Controllers": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "mailSettings": {
      // configures Cloud/local mail service class attributes 
    "mailToAddress": "admin@mycompany.com",
    "mailFromAddress": "noreply@mycompany.com"
  },
  "AllowedHosts": "*"
}
